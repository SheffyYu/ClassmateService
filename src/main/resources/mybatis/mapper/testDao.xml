<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.java.dao.testDao">
    <select id="selectAllTest" resultType="main.java.bean.testBean">
        select * from t_user;
    </select>

    <select id="selectById" resultType="main.java.bean.testBean" parameterType="String">
        select * from t_user where user_id = #{id}
    </select>
</mapper>